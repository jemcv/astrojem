---
import Social from "./Links.astro";
import { InstagramIcon, GithubIcon, RssIcon, GlobeIcon} from 'astro-feather';
---

<footer class="mt-8 flex ">
    <Social
        Icon={GithubIcon} 
        url="https://github.com"
        title="Github" 
        alt="github"
    />
    <Social
        Icon={InstagramIcon} 
        url="https://instagram.com"
        title="Instagram" 
        alt="instagram"
    />
    <Social
        Icon={RssIcon} 
        url="https://jemcv.me/rss.xml"
        title="RSS" 
        alt="rss"
    />
    <Social
        Icon={GlobeIcon} 
        url="https://jemcv.github.io/web"
        title="Archive" 
        alt="website"
    />
</footer>